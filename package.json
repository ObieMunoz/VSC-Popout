{
  "name": "popout",
  "displayName": "Popout",
  "description": "Adds a context menu option to open a file or tab in a new VS Code instance.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:popout.helloWorld"
  ],
  "main": "./extension.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/ObieMunoz/VSC-Popout"
  },
  "contributes": {
    "commands": [
      {
        "command": "extension.popout",
        "title": "Popout"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "extension.popout",
          "when": "resourceScheme == file"
        }
      ],
      "explorer/context": [
        {
          "command": "extension.popout",
          "when": "resourceScheme == file"
        }
      ],
      "editor/title/context": [
        {
          "command": "extension.popout",
          "when": "resourceScheme == file"
        }
      ]
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "pnpm run lint"
  },
  "devDependencies": {
    "@types/vscode": "^1.75.0",
    "@types/glob": "^8.0.1",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "eslint": "^8.33.0",
    "glob": "^8.1.0",
    "typescript": "^4.9.4",
    "@vscode/test-electron": "^2.2.2"
  }
}
